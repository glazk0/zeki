{
  "name": "zeki",
  "version": "2.0.0",
  "description": "A Discord bot integration of the paliapedia.com database.",
  "type": "module",
  "main": "dist/index",
  "scripts": {
    "start": "node dist/index",
    "build": "tsc && pnpm postbuild",
    "postbuild": "ncp \"src/db/migrations\" \"dist/db/migrations\"",
    "i18n": "typesafe-i18n",
    "format": "biome format src --write",
    "db:drop": "drizzle-kit drop",
    "db:generate": "drizzle-kit generate:pg",
    "db:studio": "drizzle-kit studio"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/glazk0/zeki.git"
  },
  "bugs": {
    "url": "https://github.com/glazk0/zeki/issues"
  },
  "homepage": "https://github.com/glazk0/zeki#readme",
  "keywords": [
    "discord",
    "bot",
    "palia",
    "paliapedia",
    "paliapedia.com",
    "zeki",
    "zeki-bot"
  ],
  "author": "glazk0",
  "contributors": [
    "glazk0 <glazk0@peerat.dev> (https://github.com/glazk0)"
  ],
  "license": "MIT",
  "dependencies": {
    "cheerio": "1.0.0-rc.12",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.3",
    "glob": "10.3.10",
    "node-schedule": "2.1.1",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "postgres": "^3.4.3",
    "query-string": "^8.1.0",
    "redis": "^4.6.12",
    "reflect-metadata": "^0.2.1",
    "tsyringe": "^4.8.0",
    "typesafe-i18n": "^5.26.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@types/node": "^20.11.16",
    "@types/node-schedule": "2.1.5",
    "drizzle-kit": "^0.20.12",
    "ncp": "2.0.0",
    "typescript": "^5.3.3"
  }
}
